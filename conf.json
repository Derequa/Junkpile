{
  "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
  },

  "source": {
        "include": [
          "./src/"
        ],
        "exclude": [
          "./libs/",
          "./bin/",
          "./docs/",
          "./test/",
          "main.js"
        ]
  },

  "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
  },

  "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "projectName": "Junkpile.js Engine"
  }
}
